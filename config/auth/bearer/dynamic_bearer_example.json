{
  "label": "Dynamic Bearer Example",
  "type": "bearer",
  "token": null,
  "is_dynamic": true,
  "auth_url": "https://api.example.com/auth/token",
  "auth_method": "POST",
  "auth_headers": {
    "Content-Type": "application/json"
  },
  "auth_body": "{\"username\":\"example_user\",\"password\":\"example_password\"}",
  "token_refresh_interval": 3600,
  "token_location": "data.access_token"
} 